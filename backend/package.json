{
    "name": "devops-agent-backend",
    "version": "1.0.0",
    "description": "Backend agentic system for DevOps automation",
    "main": "dist/server.js",
    "type": "module",
    "scripts": {
        "dev": "tsx watch src/server.ts",
        "build": "tsc",
        "start": "node dist/server.js",
        "test": "jest",
        "test:integration": "jest --testPathPattern=integration",
        "lint": "eslint src --ext .ts",
        "format": "prettier --write \"src/**/*.ts\""
    },
    "keywords": [
        "devops",
        "ai",
        "agent",
        "automation"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@anthropic-ai/sdk": "^0.17.1",
        "@kubernetes/client-node": "^0.20.0",
        "@octokit/rest": "^20.1.2",
        "axios": "^1.6.7",
        "cors": "^2.8.5",
        "dockerode": "^4.0.2",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "groq-sdk": "^0.3.2",
        "nanoid": "^5.0.5",
        "openai": "^4.28.0",
        "socket.io": "^4.6.1",
        "winston": "^3.11.0",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/dockerode": "^3.3.24",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.12",
        "@types/node": "^20.11.17",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^6.21.0",
        "eslint": "^8.56.0",
        "jest": "^29.7.0",
        "prettier": "^3.2.5",
        "ts-jest": "^29.1.2",
        "tsx": "^4.7.1",
        "typescript": "^5.3.3"
    }
}
